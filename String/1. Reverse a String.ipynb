    "def reverse():\n",
    "    s=input()\n",
    "    output=''\n",
    "    i=len(s)-1\n",
    "    while i >= 0:\n",
    "        output = output + s[i]\n",
    "        i -= 1\n",
    "    return output\n",
    "reverse()"
