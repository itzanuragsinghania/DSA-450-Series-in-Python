    "array=[1,2,3,4,5]\n",
    "def revlist(A,start,end):\n",
    "    while start<end:\n",
    "        A[start],A[end]=A[end],A[start]\n",
    "        start += 1\n",
    "        end -= 1\n",
    "    return A\n",
    "revlist(array,0,len(array)-1)"
